{
  "manifest_version": 3,
  "name": "Fuzzy Tabber",
  "description": "Quickly switch between tabs with a keyboard-navigatable fuzzy finder.",
  "version": "1.0",
  "permissions": ["tabs", "tabGroups"],
  "commands": {
    "_execute_action": {
      "suggested_key": "Ctrl+Shift+A",
      "description": "Open the fuzzy finder."
    }
  },
  "action": {
    "default_popup": "popup.html"
  }
}
